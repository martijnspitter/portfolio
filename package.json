	"scripts": {
		"watch:sass": "node-sass src/sass/Main.scss src/index.css -w",
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"compile:sass": "node-sass src/sass/main.scss src/style.comp.css",
		"concat:css": "concat -o src/style.concat.css src/style.comp.css",
		"prefix:css": "postcss --use autoprefixer -b 'last ten versions' src/style.concat.css -o src/style.prefix.css",
		"compress:css": "node-sass src/style.prefix.css src/index.css --output-style compressed",
		"build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"autoprefixer": "^9.8.0",
